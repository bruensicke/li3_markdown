{
	"name": "bruensicke/li3_markdown",
	"description": "Lithium PHP Markdown parser",
	"keywords": ["markdown", "lithium", "li3", "template"],
	"type": "lithium-library",
	"homepage": "https://github.com/bruensicke/li3_markdown/blob/master/README.md",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "d1rk",
			"email": "d1rk@gmx.de",
			"homepage": "https://github.com/bruensicke"
		}
	],
	"replace": {
		"joseym/li3_markdown": "*"
	},
	"suggest": {
		"UnionOfRAD/lithium": "Lithium is required for this plugin."
	},
	"support": {
		"issues": "https://github.com/bruensicke/li3_markdown/issues"
	},
	"require": {
		"php": ">=5.3",
		"composer/installers": "*"
	},
	"require-dev": {
		"evert/phpdoc-md" : "0.0.*"
	},
	"minimum-stability": "dev"
}
